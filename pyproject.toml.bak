[tool.poetry]
name = "UNI-TCC"
version = "0.1.0"
description = "Automatic Language Identification using PyTorch / Tensorflow"
authors = ["Martin <martin@hotmail.com.br>"]

[tool.poetry.dependencies]
torch = "^1.1.0"
torchvision = "^0.3.0"
jupyter = "^1.0"
pandas = "^0.25.0"
scikit-learn = "^0.21.2"
matplotlib = "^3.1"
librosa = "^0.7.0"
pyrubberband = "^0.3.0"
torchaudio = {git = "https://github.com/pytorch/audio.git", branch = "v0.2.0" }
fastai = "^1.0"
opencv-python = "^4.1"
torchsummary = "^1.5"
visdom = "^0.1.8"

[tool.poetry.dev-dependencies]
ipython = "^7.6"
autopep8 = "^1.4"
isort = "^4.3"

[build-system]
requires = ["poetry>=0.12"]
build-backend = "poetry.masonry.api"
